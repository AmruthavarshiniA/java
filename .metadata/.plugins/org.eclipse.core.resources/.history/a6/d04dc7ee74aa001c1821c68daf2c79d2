package com.testapp;

import java.util.ArrayList;

public class ShoppingCart {

	private ArrayList<Product> al;
	
	public ShoppingCart() {
		
		al=new ArrayList<Product>();
		
	}
	
	public void AddItem(Product product) {
		al.add(product);
	}
	
	public int getItemsCount() {
		return al.size();
	}
	
}
